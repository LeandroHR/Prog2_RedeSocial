
public class Postagem 
{
	private String post;
	private Comentario[] c;
	
	public Postagem(String post)
	{
		this.post = post;
	}

	public String getPost() 
	{
		return post;
	}

	public void setPost(String post) 
	{
		this.post = post;
	}

	public Comentario[] getC() 
	{
		return c;
	}

	public void setC(Comentario[] c) 
	{
		this.c = c;
	}
	
}
